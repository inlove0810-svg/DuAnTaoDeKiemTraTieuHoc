{
  "version": 2,
  "builds": [
    {
      "src": "api/generate.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.js",
      "methods": ["POST"]
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ]
}